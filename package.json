{
  "name": "music-api",
  "version": "1.0.1",
  "description": "music api colletion",
  "main": "dist/app.js",
  "browser": "dist/app.native.js",
  "scripts": {
    "dev": "nodemon --inspect controllers/app.js --exec babel-node",
    "test": "mocha -t 30000 --require @babel/register 'test/*.test.js' --exit",
    "build": "npm run build:node && npm run build:native",
    "build:node": "babel src -d dist",
    "build:native": "webpack-cli --mode=production src/app.native.js -o dist/app.native.js"
  },
  "author": "sunzongzhengwy@163.com",
  "license": "ISC",
  "dependencies": {
    "big-integer": "^1.6.26",
    "dsbridge": "^3.1.3",
    "flyio": "^0.5.7"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0-beta.40",
    "@babel/core": "^7.0.0-beta.40",
    "@babel/node": "^7.0.0-beta.40",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0-beta.40",
    "@babel/preset-env": "^7.0.0-beta.40",
    "@babel/register": "^7.0.0-beta.40",
    "mocha": "^4.0.1",
    "nodemon": "^1.12.1",
    "webpack": "^4.8.3",
    "webpack-cli": "^2.1.3"
  }
}
